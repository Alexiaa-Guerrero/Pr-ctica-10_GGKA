Práctica 10 17/10/24
Impresión de matriz 3 x 3 dada por el usuario*/

#include <stdio.h>

int main()
{
  int arr[3][3] = {{}, {}, {}};
  int i, j;
  for(i = 0; i < 3; i++)
      {
        for(j = 0; j < 3; j++)
          {
            printf("Ingrese el elemento de matriz de fila %d y columna %d", i, j);
            scanf("%d", &arr[i][j]);
          }
      }

    for(i = 0; i < 3; i++)
      {
        for(j = 0; j < 3; j++)
          {
            printf("%d ", arr[i][j]);
          }
        printf("\n");
      }
return 0;
}
